{
  "name": "electron-frame",
  "author": "Arthur Lobo",
  "license": "MIT",
  "version": "1.2.0",
  "keywords": [
    "electron",
    "frame",
    "frameless"
  ],
  "description": "Electron Frame is a simple and customizable window frame build on HTML for your electron application.",
  "scripts": {
    "build": "node ./scripts/build.js",
    "update": "yarn build && yarn add file:./npm",
    "test": "yarn update && electron ./test/main.js",
    "publish": "yarn build && cd npm && npm publish",
    "create-tag": "node ./scripts/create-tag.js"
  },
  "devDependencies": {
    "copy-directory": "https://github.com/ArthurLobopro/copy-directory",
    "electron": "17.4.11",
    "electron-frame": "file:./npm",
    "sass-compiler": "^1.3.1",
    "typescript": "^4.9.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArthurLobopro/electron-frame.git"
  }
}
